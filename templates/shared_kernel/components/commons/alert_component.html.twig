<div class="alert alert-{{ this.type }} alert-dismissible alert-label-icon label-arrow fade show" role="alert">
    <i class="mdi mdi-alert-outline label-icon"></i>
    {% if this.title != null %}<strong>{{ this.title }}</strong> - {% endif %}{{ this.message }}
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>